extends layout

block content
	h1.text-center Some Data on Jefferson City, Missouri

	.container
	ul.nav.nav-pills.nav-justified
		li.active(data-target='', data-slide-to='0')
			a.toggle-graph.graph-1.graph-link(href='#employers')
				| Top Employers
		li(data-target='#', data-slide-to='1')
			a.toggle-graph.graph-2.graph-link(href='#population')
				| Historical Population
		li(data-target='#', data-slide-to='2')
			a.toggle-graph.graph-3.graph-link(href='#climate')
				| Climate Data
		.andrew-graph#employers
			.item.active
			#top-employers-data

		.andrew-graph#population
			.item
			#population-data
				.carousel-caption

			// End Item
		.andrew-graph#climate 
			.item
			#climate-data
				.carousel-caption

	script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js")
	script(type="text/javascript", src="js/andrew.js")

	block css
		link(rel="stylesheet", type="text/css", href="css/andrew.css")